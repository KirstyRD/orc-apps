package fr.irisa.compa.gecos;

import  std.stdio.Source.*;
//import fr.irisa.common.Simulation.*;

actor generateArray () ==> uint(size=32) token :

	int i := 0;
	int val := 0;
	int INPUT_SIZE = 16*16;//1024*1024;
	
	sendData: action ==> token:[ t ]
	guard
	
		i < INPUT_SIZE
	var
		uint(size=32) t
	do
		t := val;
		if ( val < 9 ) then
			val := val + 1;
		else
			val := 0;
		end
		i := i+1;
		
	//	println("Pushing " + i);
	end

end