    <Connection dst="" dst-port="IsReadOrNewSlice" src="GenMvInfo" src-port="IsReadOrNewSlice"/>
    <Connection dst="" dst-port="Mv" src="GenMvInfo" src-port="Mv"/>
    <Connection dst="" dst-port="PocRef" src="GenMvInfo" src-port="PocRef"/>
    <Connection dst="" dst-port="PredCuSize" src="GenMvInfo" src-port="PredCuSize"/>
    <Connection dst="" dst-port="RefIdx" src="GenMvInfo" src-port="RefIdx"/>
    <Connection dst="" dst-port="RefList" src="GenMvInfo" src-port="RefList"/>
    <Connection dst="GenMvInfo" dst-port="ParserSelect" src="" src-port="ParserSelect"/>
</XDF>
